<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="keyword.bean.Comment" table="KEYWORD_COMMENT">
		<id name="id" column="ID">
			<generator class="native" /> 
		</id>
		<property name="userDailyKeywordId" column="UDKID"></property>
		<property name="userId" column="UID"></property>
		<property name="isGood" column="IS_GOOD"></property>
		<property name="comment" column="COMMENT"></property>
		<property name="date" column="DATE"></property>
	</class>
</hibernate-mapping>