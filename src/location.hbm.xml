<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="keyword.bean.Location" table="KEYWORD_LOCATION">
		<id name="id" column="ID">
			<generator class="native" /> 
		</id>
		<property name="location" column="LOCATION"></property>
		<property name="country" column="COUNTRY"></property>
		<property name="state" column="STATE"></property>
		<property name="city" column="CITY"></property>
		<property name="district" column="DISTRICT"></property>
		<property name="street" column="STREET"></property>
		<property name="lagtitute" column="LAGTITUTE"></property>
		<property name="longtitute" column="LONGTITUTE"></property>
	</class>
</hibernate-mapping>