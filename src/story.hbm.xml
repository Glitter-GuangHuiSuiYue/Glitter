<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="keyword.bean.Story" table="KEYWORD_STORY">
		<id name="id" column="ID">
			<generator class="native" /> 
		</id>
		<property name="userDailyKeywordId" column="UDKID"></property>
		<property name="story" column="STORY"></property>
	</class>
</hibernate-mapping>